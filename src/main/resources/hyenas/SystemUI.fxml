<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="BoxPane" fx:id="boxPane" styleClass="default-background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hyenas.SystemUIController">
    <children>
        <AnchorPane prefHeight="60">
            <children>
                <MenuButton fx:id="options" layoutX="8.0" layoutY="14.0" mnemonicParsing="false" text="Options">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Settings" />
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </MenuButton>
                <Button fx:id="marketPlaceButton" layoutX="105.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToMarketplace" text="Marketplace"/>
            </children>
        </AnchorPane>
        <BorderPane>
            <center>
                <Pane id="SystemPane" fx:id="systemPane" styleClass="system-background"></Pane>
            </center>
        </BorderPane>
        
        <AnchorPane id="AnchorPane" fx:id="anchor" prefHeight="60">
            <children>
                <Button fx:id="backButton" mnemonicParsing="false" onAction="#goBack" text="Back" />
            </children>
        </AnchorPane>
    </children>
</VBox>
